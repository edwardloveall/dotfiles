"*":
  editor:
    invisibles: {}
    softWrap: true
    fontFamily: "Inconsolata"
    scrollPastEnd: true
    fontSize: 16
  linter:
    lintOnChange: false
    ignoredLinterErrors: [
      "rubocop"
    ]
    showErrorPanel: false
    statusIconScope: "File"
    lintOnFly: false
    gutterPosition: "Left"
    gutterEnabled: false
    showErrorTabFile: false
    showErrorTabProject: false
  "exception-reporting":
    userId: "05769e4a-414c-0234-3c4f-848d932dafcd"
  welcome:
    showOnStartup: false
  core:
    themes: [
      "one-light-ui"
      "tomorrow"
    ]
    disabledPackages: [
      "metrics"
      "background-tips"
      "archive-view"
      "autocomplete-snippets"
      "bookmarks"
      "keybinding-resolver"
      "language-toml"
      "wrap-guide"
      "deprecation-cop"
      "autocomplete-atom-api"
    ]
    audioBeep: false
  "linter-rubocop":
    command: "/Users/edwardloveall/.gem/ruby/latest/bin/rubocop"
  "one-light-ui": {}
  "autocomplete-plus":
    confirmCompletion: "enter"
  "the-closer":
    closeWindowTogetherWithLastTab: false
  "tree-view":
    hideVcsIgnoredFiles: true
  "color-picker":
    uppercaseColorValues: true
    preferredFormat: "HEX"
    randomColor: false
  "linter-scss-lint":
    executablePath: "/usr/local/bin/scss-lint"
