"*":
  "autocomplete-plus":
    confirmCompletion: "enter"
  "color-picker":
    preferredFormat: "HEX"
    randomColor: false
    uppercaseColorValues: true
  core:
    allowPendingPaneItems: false
    audioBeep: false
    disabledPackages: [
      "metrics"
      "background-tips"
      "archive-view"
      "autocomplete-snippets"
      "bookmarks"
      "keybinding-resolver"
      "language-toml"
      "wrap-guide"
      "deprecation-cop"
      "autocomplete-atom-api"
      "linter-scss-lint"
      "language-gfm"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "tomorrow"
    ]
  editor:
    fontFamily: "Mononoki"
    invisibles: {}
    scrollPastEnd: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "05769e4a-414c-0234-3c4f-848d932dafcd"
  linter:
    gutterEnabled: false
    gutterPosition: "Left"
    ignoredLinterErrors: [
      "rubocop"
    ]
    lintOnChange: false
    lintOnFly: false
    showErrorPanel: false
    showErrorTabFile: false
    showErrorTabProject: false
    statusIconScope: "File"
  "linter-jshint":
    executablePath: "/usr/local/bin/jslint"
  "linter-rubocop":
    command: "/usr/local/bin/rubocop"
  "linter-scss-lint":
    executablePath: "/usr/local/bin/scss-lint"
  "one-light-ui": {}
  tabs:
    addNewTabsAtEnd: true
  "the-closer":
    closeWindowTogetherWithLastTab: false
  "tree-view": {}
  welcome:
    showOnStartup: false
